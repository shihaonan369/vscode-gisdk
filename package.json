{
  "name": "vscode-gisdk",
  "displayName": "GISDK",
  "description": "",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "publisher": "shihaonan369",
  "PAT": "zkvupjb2jaxjwt4mafbj2skglm225pi54irprerpy2tryfpah2zq",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:gisdk",
    "workspaceContains:**/*.rsc",
    "workspaceContains:**/*.gisdk",
    "workspaceContains:**/*.model",
    "workspaceContains:**/*.scenarios"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-gisdk.selectCompiler",
        "title": "GISDK: Select Compiler"
      }
    ],
    "languages": [
      {
        "id": "gisdk",
        "aliases": ["GISDK"],
        "extensions": [
          ".rsc",
          ".gisdk",
          ".model",
          ".scenarios"
        ]
      }
    ],
    "problemMatchers": [
      {
        "name": "rscc",
        "fileLocation": "absolute",
        "pattern": {
          "regexp": "^(.*)\\((\\d+, \\d+)\\):\\s(.*)$",
          "file": 1,
          "location": 2,
          "message": 3
        }
      }
    ],
    "taskDefinitions": [
      {
        "type": "rscc",
        "required": [
          "args"
        ],
        "properties": {
          "compilerPath": {
            "type": "string",
            "description": "You can set this property to path of rscc if you want to compile with custom rscc."
          },
          "args": {
            "type": "array",
            "description": "The arguments passed to rscc."
          }
        }
      }
    ],
    "debuggers": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-debugadapter": "^1.41.1",
    "vscode-debugadapter-testsupport": "^1.41.0",
    "vscode-debugprotocol": "^1.41.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "regedit": "^3.0.3"
  }
}
